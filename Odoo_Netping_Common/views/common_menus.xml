	<!-- Discuss -->
	<record model="ir.ui.menu" id="mail.menu_root_discuss">
		<field name="groups_id" eval="[(6,0,[ref('Odoo_Netping_Common.group_show_hidden_menus')])]"/>
	</record>
	
	<!-- Contacts -->
	<record model="ir.ui.menu" id="contacts.menu_contacts">
		<field name="groups_id" eval="[(6,0,[ref('Odoo_Netping_Common.group_show_hidden_menus')])]"/>
	</record>
	
	<!-- Link Tracker -->
	<record model="ir.ui.menu" id="utm.menu_link_tracker_root">
		<field name="groups_id" eval="[(6,0,[ref('Odoo_Netping_Common.group_show_hidden_menus')])]"/>
	</record>
	
	<!-- Quality -->
	<record model="ir.ui.menu" id="quality_control.menu_quality_root">
		<field name="groups_id" eval="[(6,0,[ref('Odoo_Netping_Common.group_show_hidden_menus')])]"/>
	</record>
	
	
	<!-- Override Menu Items (Hide Menues For group_uses (Internal Users) Users) -->
  
